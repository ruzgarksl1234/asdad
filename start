-- Fake Code for Cool Startup

-- Cool startup simulation with print statements only
function coolStartup()
    print("Starting the system...")
    socket.sleep(0.1)
    
    print("Initializing core components...")
    socket.sleep(0.1)
    
    print("Loading module: core")
    socket.sleep(0.1)
    
    print("Loading module: network")
    socket.sleep(0.1)
    
    print("Loading module: GUI")
    socket.sleep(0.1)
    
    print("Loading module: database")
    socket.sleep(0.1)
    
    print("Modules loaded successfully.")
    socket.sleep(0.1)
    
    print("Setting up configuration...")
    socket.sleep(0.1)
    
    print("Configuration: Network = enabled")
    socket.sleep(0.1)
    
    print("Configuration: Graphics = high")
    socket.sleep(0.1)
    
    print("Configuration: Debug mode = false")
    socket.sleep(0.1)
    
    print("Running diagnostics...")
    socket.sleep(0.1)
    
    print("Diagnostics: CPU = OK")
    socket.sleep(0.1)
    
    print("Diagnostics: Memory = OK")
    socket.sleep(0.1)
    
    print("Diagnostics: Disk = OK")
    socket.sleep(0.1)
    
    print("Finalizing startup...")
    socket.sleep(0.1)
    
    print("System startup complete!")
end

coolStartup()
